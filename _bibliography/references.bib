journal = {Emotion (Washington, D.C.)},
keywords = {Anxiety,Anxiety: physiopathology,Anxiety: psychology,Cognition,Electromyography,Emotions,Female,Functional Laterality,Humans,Linear Models,Male,Memory,Prefrontal Cortex,Psychometrics,Reaction Time,Space Perception,United States},
month = mar,
number = {1},
pages = {40--61},
pmid = {16637749},
title = {{Anxiety selectively disrupts visuospatial working memory.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/16637749},
volume = {6},
year = {2006}
}
@article{Shimamura2000,
