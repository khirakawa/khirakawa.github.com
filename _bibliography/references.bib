pages = {1295--306},
pmid = {9770223},
title = {{Feature binding, attention and object perception.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=1692340\&tool=pmcentrez\&rendertype=abstract},
