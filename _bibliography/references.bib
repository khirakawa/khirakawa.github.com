volume = {53},
year = {1993}
}
@article{Conway2001,
abstract = {Wood and Cowan (1995) replicated and extended Moray's (1959) investigation of the cocktail party phenomenon, which refers to a situation in which one can attend to only part of a noisy environment, yet highly pertinent stimuli such as one's own name can suddenly capture attention. Both of these previous investigations have shown that approximately 33\% of subjects report hearing their own name in an unattended, irrelevant message. Here we show that subjects who detect their name in the irrelevant message have relatively low working-memory capacities, suggesting that they have difficulty blocking out, or inhibiting, distracting information.},
