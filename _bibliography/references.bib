keywords = {Adult,Analysis of Variance,Brain Mapping,Electromyography,Electromyography: methods,Evoked Potentials, Motor,Evoked Potentials, Motor: physiology,Female,Functional Laterality,Functional Laterality: physiology,Humans,Male,Movement,Movement: physiology,Pattern Recognition, Visual,Pattern Recognition, Visual: physiology,Photic Stimulation,Photic Stimulation: methods,Psychomotor Performance,Psychomotor Performance: physiology,Transcranial Magnetic Stimulation,Young Adult},
month = dec,
number = {14},
pages = {3074--8},
pmid = {19615389},
title = {{Broken affordances, broken objects: a TMS study.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/19615389},
volume = {47},
year = {2009}
}
